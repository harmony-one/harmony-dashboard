    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.2.3",
    "http-server": "^0.12.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "npx": "^10.2.2",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  }
